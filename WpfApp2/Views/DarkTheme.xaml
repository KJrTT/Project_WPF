<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="WindowBackgroundColor">#FF232323</Color>
    <Color x:Key="TextColor">#FFFFFFFF</Color>
    <Color x:Key="CalendarCellBackgroundColor">#FF111111</Color>
    <Color x:Key="CalendarCellDateBackgroundColor">#FF2C2C2C</Color>
    <Color x:Key="CalendarCellBorderColor">#FFFFFFFF</Color>
    <Color x:Key="CalendarCellTextColor">#FFFFFFFF</Color>
    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="{StaticResource WindowBackgroundColor}"/>
    <SolidColorBrush x:Key="TextBrush" Color="{StaticResource TextColor}"/>
    <SolidColorBrush x:Key="CalendarCellBackgroundBrush" Color="{StaticResource CalendarCellBackgroundColor}"/>
    <SolidColorBrush x:Key="CalendarCellDateBackgroundBrush" Color="{StaticResource CalendarCellDateBackgroundColor}"/>
    <SolidColorBrush x:Key="CalendarCellBorderBrush" Color="{StaticResource CalendarCellBorderColor}"/>
    <SolidColorBrush x:Key="CalendarCellTextBrush" Color="{StaticResource CalendarCellTextColor}"/>
    <!-- Добавьте другие стили и цвета по необходимости -->
</ResourceDictionary> 